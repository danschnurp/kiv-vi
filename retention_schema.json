{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "bachelor": {
      "type": "object",
      "properties": {
        "RU": { "$ref": "#/definitions/countryData" },
        "UAS": { "$ref": "#/definitions/countryData" },
        "RU+UAS": { "$ref": "#/definitions/countryData" }
      },
      "required": ["RU", "UAS", "RU+UAS"]
    }
  },
  "required": ["bachelor"],
  "definitions": {
    "countryData": {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "year": { "type": "string" },
            "total": { "type": "number" },
            "male": { "type": "number" },
            "female": { "type": "number" },
            "category": { "type": "integer" }
          },
          "required": ["year", "total", "male", "female", "category"]
        }
      }
    }
  }
}
